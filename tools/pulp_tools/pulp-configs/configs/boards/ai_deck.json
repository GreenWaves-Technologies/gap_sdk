{
  "@includes@": [ "chips/gap_rev1/gap_rev1.json" ],

  "board": {
    "name": "ai_deck",
    "devices": {
      "flash": {
        "@include@" : "devices/hyperflash.json",
        "cs": 1
      },
      "ram": {
        "@include@" : "devices/hyperram.json",
        "cs": 0
      },
      "camera": {
        "@include@": "devices/camera.json",
        "interface": "cpi0",
        "ctrl_interface": "i2c0"
      },
      "wifi": {
        "@include@": "devices/nina_w10.json",
        "interface": "spim1",
        "cs": 0,
        "gpio_ready": "gpio18"
      }
    }
  },

  "runner": {
    "config": {
      "boot-mode": "rom_hyper",
      "flash_type": "hyper",
      "default_profile": "hyper"
    }
  },

  "debug_bridge": {
    "config": {
      "cable": {
        "type": "ftdi",
        "system_reset_gpio": 9,
        "jtag_reset_gpio": 8,
        "reverse_reset": false
      }
    }
  }

}
