{
  "vp_class": "pulp/soc",

  "vp_comps": [ "soc_ico", "fc", "l2_priv0", "l2_priv1", "l2_shared_0", "l2_shared_1", "l2_shared_2", "l2_shared_3", "plt_loader", "fc_itc", "fll0", "fll1", "fll2", "fll_ctrl", "apb_soc_ctrl", "stdout", "timer", "soc_eu", "udma", "xne"],

  "vp_bindings": [
    ["fc->fetch", "soc_ico->fc_fetch"],
    ["fc->data", "soc_ico->fc_data"],
    ["soc_ico->stdout", "stdout->in"],
    ["soc_ico->l2_priv0", "l2_priv0->in"],
    ["soc_ico->l2_priv1", "l2_priv1->in"],
    ["soc_ico->l2_shared_0", "l2_shared_0->in"],
    ["soc_ico->l2_shared_1", "l2_shared_1->in"],
    ["soc_ico->l2_shared_2", "l2_shared_2->in"],
    ["soc_ico->l2_shared_3", "l2_shared_3->in"],
    ["soc_ico->fll0", "fll0->in"],
    ["soc_ico->fll1", "fll1->in"],
    ["soc_ico->fll2", "fll2->in"],
    ["soc_ico->apb_soc_ctrl", "apb_soc_ctrl->in"],
    ["soc_ico->fc_itc", "fc_itc->in"],
    ["soc_ico->fc_timer", "timer->in"],
    ["soc_ico->soc_eu", "soc_eu->in"],
    ["soc_ico->udma", "udma->in"],
    ["soc_ico->xne", "xne->in"],
    ["xne->out", "soc_ico->hwpe_in"],
    ["plt_loader->out", "soc_ico->debug"]
  ],

  "plt_loader": {
    "vp_class": "utils/loader",
    "binaries": []
  },

  "soc_ico": {

    "vp_class": "pulp/soc_ico",

    "gv_class": "pulp.Soc_Ico_v2.Soc_Ico",

    "nb_l2_shared_banks": 4,

    "ll_ico": {
      "bandwidth": 4
    },

    "hb_ico": {
      "nb_slaves": 4,
      "interleaving_bits": 2,
      "stage_bits": 2,
      "remove_offset": "0x1c010000"
    },

    "apb_ico": {
      "bandwidth": 4
    },

    "fc_fetch_ico": {
      "bandwidth": 4
    },

    "fc_data_ico": {
      "bandwidth": 4
    },

    "apb": {
      "base"  : "0x1A100000",
      "size"  : "0x00100000"
    },

    "l2_priv0": {
      "base"  : "0x1C000000",
      "size"  : "0x00006000",
      "alias_base"  : "0x00000000"
    },

    "l2_scm": {
      "base"  : "0x1C006000",
      "size"  : "0x00002000"
    },

    "l2_priv1": {
      "base"  : "0x1C008000",
      "size"  : "0x00008000"
    },

    "l2_shared": {
      "base"  : "0x1C010000",
      "size"  : "0x00070000"
    },

    "rom": {
      "base"  : "0x1A000000",
      "size"  : "0x00010000"
    },

    "external_binding": {
      "rcv_fd" : -1,
      "snd_fd" : -1
    },

    "peripherals_base"  : "0x1A100000",

    "peripherals": {

      "xne": {
        "offset": "0x0000c000",
        "size"  : "0x00001000"
      },

      "stdout": {
        "offset": "0x0000f000",
        "size"  : "0x00001000"
      },

      "apb_soc_ctrl": {
        "offset": "0x00004000",
        "size"  : "0x00001000"
      },

      "udma": {
        "offset": "0x00002000",
        "size"  : "0x00002000"
      },

      "fc_itc": {
        "offset": "0x00009800",
        "size"  : "0x00000800"
      },

      "soc_eu": {
        "offset": "0x00006000",
        "size"  : "0x00001000"
      },

      "fc_debug": {
        "offset": "0x00010000",
        "size"  : "0x00008000"
      },

      "gpio": {
        "offset": "0x00001000",
        "size"  : "0x00001000"
      },

      "pmu": {
        "offset": "0x00007000",
        "size"  : "0x00000800"
      },

      "fc_timer": {
        "offset": "0x0000B000",
        "size"  : "0x00001000"
      }
    }
  },

  "l2": {
    "is_partitioned": true,
    "nb_shared_banks": 4,
    "map_base": "0x1C000000",
    "map_size": "0x00080000"
  },

  "l2_priv0": {
    "size": 24576,
    "map_base": "0x1C000000",
    "map_size": "0x00006000",
    "vp_class": "memory/memory"
  },
  
  "l2_scm": {
    "size": 8192,
    "map_base": "0x1C006000",
    "map_size": "0x00008000"
  },
  
  "l2_priv1": {
    "size": 32768,
    "map_base": "0x1C008000",
    "map_size": "0x00008000",
    "vp_class": "memory/memory"
  },
  
  "l2_shared": {
    "nb_banks": 4,
    "size": 458752,
    "map_base": "0x1C010000",
    "map_size": "0x00FF0000",
    "vp_class": "memory/memory"
  },
  
  "l2_shared_0": {
    "size": 114688,
    "vp_class": "memory/memory"
  },
  
  "l2_shared_1": {
    "size": 114688,
    "vp_class": "memory/memory"
  },
  
  "l2_shared_2": {
    "size": 114688,
    "vp_class": "memory/memory"
  },
  
  "l2_shared_3": {
    "size": 114688,
    "vp_class": "memory/memory"
  },
  
  "fc_ico": {
    "l2_alias" : true
  },

  "fll0": {
    "includes"  : ["../fll_v1.json"]
  },

  "fll1": {
    "includes"  : ["../fll_v1.json"]
  },

  "fll2": {
    "includes"  : ["../fll_v1.json"]
  },

  "rom": {
    "includes"  : ["../rom_v2.json"],
    "size"      : 8192,
    "map_base"  : "0x1A000000",
    "map_size"  : "0x00010000",
    "vp_class": "memory/memory"
  },

  "fll_ctrl": {
    "version": 3,
    "vp_class": "pulp/fll/fll_ctrl",
    "gv_class": "pulp.Fll_ctrl.fll_ctrl"
  },

  "peripherals_base": "0x1A100000",
  "stdout": {
    "includes"  : ["../stdout_v3.json"]
  },
  "gpio": {
    "includes"  : ["../gpio_v2.json"]
  },
  
  "xne": {
    "includes"  : ["../xne_v1.json"]
  },

  "pmu": {
    "includes"  : ["../pmu_v2.json"]
  },
  "udma": {
    "includes"  : ["../udma_v2.1.json"]
  },
  "timer": {
    "includes"  : ["../timer_v2.json"]
  },
  "soc_eu": {
    "includes"  : ["../soc_eu_v1.json"]
  },
  "uart": {
    "version": 1
  },
  "fc_itc": {
    "includes"  : ["../itc_v1.json"]
  },
  "fc_debug": {
    "version": 1
  },

  "fc_events": {
    "evt_sw_event0"       : 0,
    "evt_sw_event2"       : 2,
    "evt_sw_event1"       : 1,
    "evt_sw_event3"       : 3,
    "evt_sw_event4"       : 4,
    "evt_sw_event5"       : 5,
    "evt_sw_event6"       : 6,
    "evt_sw_event7"       : 7,

    "evt_timer0"          : 10,
    "evt_timer1"          : 11,

    "evt_clkref"          : 14,
    "evt_gpio"            : 15,
    "evt_rtc"             : 16,

    "evt_adv_timer0"      : 17,
    "evt_adv_timer1"      : 18,
    "evt_adv_timer2"      : 19,
    "evt_adv_timer3"      : 20,

    "evt_cluster_not_busy": 21,
    "evt_cluster_pok"     : 22,
    "evt_cluster_cg_ok"   : 23,
    "evt_picl_ok"         : 24,
    "evt_scu_ok"          : 25,

    "evt_soc_evt"         : 26,
    "evt_queue_error"     : 29
  },

  "soc_events_ids": {
    "soc_evt_spim0_rx"        : 0,
    "soc_evt_spim0_tx"        : 1,
    "soc_evt_spim1_rx"        : 2,
    "soc_evt_spim1_tx"        : 3,
    "soc_evt_hyper0_rx"       : 4,
    "soc_evt_hyper0_tx"       : 5,
    "soc_evt_uart0_rx"        : 6,
    "soc_evt_uart0_tx"        : 7,
    "soc_evt_i2c0_rx"         : 8,
    "soc_evt_i2c0_tx"         : 9,
    "soc_evt_i2c1_rx"         : 10,
    "soc_evt_i2c1_tx"         : 11,
    "soc_evt_i2s_ch0"         : 12,
    "soc_evt_i2s_ch1"         : 13,
    "soc_evt_cam0"            : 14,
    "soc_evt_spim0_eot"       : 22,
    "soc_evt_l2l2_eot"        : 23,
    "soc_evt_uart_eot"        : 25,
    "soc_evt_i2c0_extra"      : 26,
    "soc_evt_i2c1_extra"      : 27,
    "soc_evt_i2s_extra"       : 28,
    "soc_evt_cam0_eot"        : 29,
    "soc_evt_cluster_pok"     : 31,
    "soc_evt_cluster_not_busy": 34,
    "soc_evt_cluster_cg_ok"   : 35,
    "soc_evt_picl_ok"         : 36,
    "soc_evt_scu_ok"          : 37,
    "soc_evt_adv_timer0"      : 38,
    "soc_evt_adv_timer1"      : 39,
    "soc_evt_adv_timer2"      : 40,
    "soc_evt_adv_timer3"      : 41,
    "soc_evt_gpio"            : 42,
    "soc_evt_rtc_apb"         : 43,
    "soc_evt_rtc"             : 44,
    "soc_evt_ref_clock"       : 56,

    "soc_evt_sw_first"        : 48,
    "soc_evt_sw_nb"           : 8,

    "soc_evt_pmu0"            : 31
  },

  "apb_soc_ctrl": {
    "includes"  : ["../apb_soc_v4.json"],
    "vp_class": "pulp/chips/pulpissimo/apb_soc"
  }
}
