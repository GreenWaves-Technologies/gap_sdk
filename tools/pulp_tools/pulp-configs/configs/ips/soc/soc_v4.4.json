    "evt_cluster_not_busy": 21,
    "evt_cluster_pok"     : 22,
    "evt_cluster_cg_ok"   : 23,
    "evt_picl_ok"         : 24,
    "evt_scu_ok"          : 25,

    "evt_soc_evt"         : 26,
