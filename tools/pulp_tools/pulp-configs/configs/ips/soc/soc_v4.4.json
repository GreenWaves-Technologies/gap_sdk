{
  "soc_ico": {

    "gv_class": "pulp.Soc_Ico_v2.Soc_Ico",

    "nb_l2_shared_banks": 4,

    "apb": {
      "base"  : "0x1A100000",
      "size"  : "0x00100000"
    },

    "l2_priv0": {
      "base"  : "0x1C000000",
      "size"  : "0x00004000"
    },

    "l2_priv1": {
      "base"  : "0x1C004000",
      "size"  : "0x00004000"
    },

    "l2_shared": {
      "base"  : "0x1C008000",
      "size"  : "0x00018000"
    },

    "rom": {
      "base"  : "0x1A000000",
      "size"  : "0x00010000"
    },

    "external_binding": {
      "rcv_fd" : -1,
      "snd_fd" : -1
    },

    "peripherals_base"  : "0x1A100000",

    "peripherals": {

      "stdout": {
        "offset": "0x0000f000",
        "size"  : "0x00001000"
      },

      "apb_soc_ctrl": {
        "offset": "0x00004000",
        "size"  : "0x00001000"
      },

      "udma": {
        "offset": "0x00002000",
        "size"  : "0x00002000"
      },

      "fc_itc": {
        "offset": "0x00009800",
        "size"  : "0x00000800"
      },

      "soc_eu": {
        "offset": "0x00006000",
        "size"  : "0x00001000"
      },

      "fc_debug": {
        "offset": "0x00010000",
        "size"  : "0x00008000"
      },

      "gpio": {
        "offset": "0x00001000",
        "size"  : "0x00001000"
      },

      "pmu": {
        "offset": "0x00007000",
        "size"  : "0x00000800"
      },

      "fc_timer": {
        "offset": "0x0000B000",
        "size"  : "0x00001000"
      }
    }
  },

  "l2": {
    "is_partitioned": true,
    "nb_shared_banks": 4,
    "map_base": "0x1C000000",
    "map_size": "0x00080000"
  },

  "l2_priv0": {
    "size": 16384,
    "map_base": "0x1C000000",
    "map_size": "0x00004000"
  },
  
  "l2_priv1": {
    "size": 16384,
    "map_base": "0x1C004000",
    "map_size": "0x00004000"
  },
  
  "l2_shared": {
    "nb_banks": 4,
    "size": 65536,
    "map_base": "0x1C008000",
    "map_size": "0x00018000"
  },
  
  "gpio": {
    "@includes@"  : ["gpio_v2.json"]
  },
  "fc": {
    "@includes@"  : ["zeroriscy.json"],
    "cluster_id": 31,
    "core_id"   : 0
  },

  "fc_ico": {
    "l2_alias" : true
  },

  "rom": {
    "@includes@"  : ["rom_v2.json"],
    "size"      : 8192,
    "map_base"  : "0x1A000000",
    "map_size"  : "0x00010000"
  },

  "peripherals_base": "0x1A100000",
  "stdout": {
    "@includes@"  : ["stdout_v3.json"]
  },
  "apb_soc_ctrl": {
    "@includes@"  : ["apb_soc_v3.json"]
  },
  "pmu": {
    "@includes@"  : ["pmu_v2.json"]
  },
  "udma": {
    "@includes@"  : ["udma_v2.2.json"]
  },
  "timer": {
    "@includes@"  : ["timer_v2.json"]
  },
  "soc_eu": {
    "@includes@"  : ["soc_eu_v1.json"]
  },
  "uart": {
    "version": 1
  },
  "fc_itc": {
    "@includes@"  : ["itc_v1.json"]
  },
  "fc_debug": {
    "version": 1
  },

  "fc_events": {
    "evt_sw_event0"       : 0,
    "evt_sw_event2"       : 2,
    "evt_sw_event1"       : 1,
    "evt_sw_event3"       : 3,
