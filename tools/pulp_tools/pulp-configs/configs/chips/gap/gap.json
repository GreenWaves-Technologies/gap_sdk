{
  "config_type": "generator",
  "@generator@": "generators/v1/config_gen",

  "board": {
    "devices": {
      "uart": {
        "@includes@": ["devices/uart_tb.json"],
        "interface": "uart0"
      },
      "jtag_proxy": {
        "@includes@": ["devices/jtag_proxy.json"]
      }
    }
  },

  "debug_bridge": {
    "config": {
      "cable": {
        "system_reset_gpio": 4,
        "jtag_reset_gpio": 6,
        "reverse_reset": true
      }
    }
  },

  "rt": {
    "config": {
      "bsp": true
    }
  },


  "chip": "gap",
  "chip_family": "gap",
    
  "padframe": {
    "content": "chips/gap/padframe.json"
  },

  "cluster0": {
    "pe0": {
      "boot_addr"   : "0x1C000080"
    },
    "pe1": {
      "boot_addr"   : "0x1C000080"
    },
    "pe2": {
      "boot_addr"   : "0x1C000080"
    },
    "pe3": {
      "boot_addr"   : "0x1C000080"
    },
    "pe4": {
      "boot_addr"   : "0x1C000080"
    },
    "pe5": {
      "boot_addr"   : "0x1C000080"
    },
    "pe6": {
      "boot_addr"   : "0x1C000080"
    },
    "pe7": {
      "boot_addr"   : "0x1C000080"
    }
  },

  "cluster": {
    "base": "0x10000000",
    "alias": "0x00000000",
    "size": "0x00400000",
    "core": "ri5ky_v2_cplx",
    "version": 5,
    "nb_cluster": 1,
    "nb_pe": 8,
    
    "icache": {
      "config": {
        "nb_sets_bits": 6,
        "nb_ways_bits": 2,
        "line_size_bits": 4
      }
    },

    "pe": {
      "irq": [
        null       , null       , null         , null,
        null       , null       , null         , null,
        "dma_0"    , "dma_1"    , "timer_0"    , "timer_1",
        "acc_0"    , "acc_1"    , "acc_2"      , "acc_3",
        "eu_bar"   , "eu_mutex" , "eu_dispatch", null,
        null       , null       , "dma_ext"    , null,
        "soc_fifo" , null       , null         , null,
        null       , null       , null         , null
      ]
    },

    "l1": {
      "base": "0x10000000",
      "size": "0x00010000",
      "banking_factor": 2
    },

    "peripherals": {

      "base": "0x10200000",
      "size": "0x00200000",

      "alias": {
        "base": "0x00200000",
        "size": "0x00200000"
      },

      "cluster_ctrl": {
        "base": "0x10200000",
        "size": "0x00000400"
      },

      "hwce": {
        "base": "0x10201000",
        "size": "0x00000400",
        "version": 4
      },

      "timer": {
        "base": "0x10200400",
        "size": "0x00000400"
      },

      "event_unit": {
        "base": "0x10200800",
        "size": "0x00000800"
      },

      "icache_ctrl": {
        "base": "0x10201400",
        "size": "0x00000400"
      },

      "dma": {
        "version": 6,
        "base": "0x10201800",
        "size": "0x00000400"
      },

      "dbg_unit": {
        "base": "0x10300000",
        "size"  : "0x00008000"
      },

      "cluster_ico": {}
    },

    "demux_peripherals":
    {
      "base": "0x10204000",

      "event_unit": {
        "base": "0x10204000",
        "size": "0x00000400"
      },
      "dma": {
        "base": "0x10204400",
        "size": "0x00000400"
      }
    }
  },

  "soc": {
    
    "base": "0x1A000000",
    "size": "0x06000000",

    "fc": {
      "core"        : "ri5ky_v2_cplx",
      "cluster_id"  : 32,
      "core_id"     : 0,
      "fetch_enable": true,
      "boot_addr"   : "0x1A000000"
    },

    "rom": {
      "version": 2,
      "base": "0x1A000000",
      "size": "0x00002000",
      "config": {
        "@eval@": {
          "stim_file": "os.path.join(os.environ.get('INSTALL_DIR'), 'python', 'pulp', 'chips', 'gap', 'rom.bin')"
        }
      }
    },

    "pulp_tap": {
      "config": {
        "@includes@": [ "ips/debug/pulp_tap.json" ]
      }
    },

    "taps": [ "pulp_tap" ],

    "l2": {
      "base": "0x1C000000",
      "size": "0x00080000",

      "shared": {
        "nb_banks"     : 4,
        "interleaving_bits" : 2,
        "base"         : "0x1C000000",
        "size"         : "0x00000000"
      }
    },

    "fc_ico": {
      "base": "0x1B000000",
      "size": "0x00400000",

      "alias": {
        "base"         : "0x00000000",
        "size"         : "0x00400000"
      },

      "peripherals": {
        "fc_eu_alias": {
          "base"         : "0x1B204000",
          "size"         : "0x00001000"
        },

        "fc_eu": {
          "version"      : 3,
          "base"         : "0x1B200800",
          "alias"        : "0x00200800",
          "size"         : "0x00000800",
          "irq": {
            "evt_sw_event0"       : 0,
            "evt_sw_event2"       : 2,
            "evt_sw_event1"       : 1,
            "evt_sw_event3"       : 3,
            "evt_sw_event4"       : 4,
            "evt_sw_event5"       : 5,
            "evt_sw_event6"       : 6,
            "evt_sw_event7"       : 7,

            "evt_timer0"          : 10,
            "evt_timer1"          : 11,

            "evt_clkref"          : 14,

            "evt_adv_timer0"      : 17,
            "evt_adv_timer1"      : 18,
            "evt_adv_timer2"      : 19,
            "evt_adv_timer3"      : 20,

            "evt_cluster_not_busy": 21,
            "evt_cluster_pok"     : 22,
            "evt_cluster_cg_ok"   : 23,
            "evt_picl_ok"         : 24,
            "evt_scu_ok"          : 25,

            "evt_soc_evt"         : 26,
            "evt_queue_error"     : 29
          }
        },

        "fc_tcdm": {
          "base"         : "0x1B000000",
          "size"         : "0x00010000"
        },

        "fc_timer": {
          "version"      : 2,
          "base"         : "0x1B200400",
          "size"         : "0x00001000"
        },

        "fc_icache": {
          "version"      : 2,
          "base"         : "0x1B201400",
          "size"         : "0x00001400",
          "config": {
            "nb_ports" : 1,
            "nb_sets_bits": 4,
            "nb_ways_bits": 2,
            "line_size_bits": 4
          }
        },

        "fc_dbg_unit": {
          "version"      : 1,
          "base"         : "0x1B300000",
          "size"         : "0x00008000"
        }


      }
    },

    "peripherals": {

      "base": "0x1A100000",
      "size": "0x00100000",

      "flls": {
        "fll": {
          "version"      : 1,
          "base"         : "0x1A100000",
          "size"         : "0x00000010",
          "targets"      : [ "soc", "periph" ]
        },

        "fll_cluster": {
          "version"      : 1,
          "base"         : "0x1A100010",
          "size"         : "0x00000010",
          "targets"      : [ "cluster" ]
        }
      },

      "fll_ctrl": {
        "version"      : 3
      },

      "gpio": {
        "version"      : 2,
        "base"         : "0x1A101000",
        "size"         : "0x00001000"
      },

      "udma": {
        "content"      : "chips/gap/udma.json",
        "base"         : "0x1A102000",
        "size"         : "0x00002000"
      },

      "apb_soc_ctrl": {
        "base"         : "0x1A104000",
        "size"         : "0x00001000",
        "has_jtag_reg" : true,
        "has_pmu_bypass" : true
      },

      "soc_eu": {
        "version"      : 1,
        "base"         : "0x1A106000",
        "size"         : "0x00001000"
      },

      "pmu": {
        "content"      : "chips/gap/pmu.json",
        "version"      : 1,
        "base"         : "0x1A107000",
        "size"         : "0x00000800"
      },

      "rtc": {
        "base"         : "0x1A108000",
        "size"         : "0x00001000",
        "config": {
          "apb_irq_soc_event": 43,
          "irq_soc_event"    : 44,
          "calendar_time"    : "0x00124808",
          "calendar_date"    : "0x00171228"
        }
      },

      "efuse": {
        "version"      : 1,
        "base"         : "0x1A109000",
        "size"         : "0x00001000"
      },

      "stdout": {
        "version"      : 3,
        "base"         : "0x1A110000",
        "size"         : "0x00002000"
      }
    }
  },

  "soc_events": {
