{
  "config_type": "generator",
  "generator": "generators/v1/config_gen",

  "debug_bridge": {
    "config": {
      "cable": {
        "system_reset_gpio": 9,
        "jtag_reset_gpio": 8,
        "reverse_reset": false
      }
    }
  },

  "board": {
    "devices": {
      "spiflash": {
        "@includes@": ["devices/spiflash_s26ks512.json"],
        "interface": "spim0",
        "cs": 0
      },
      "uart": {
        "@includes@": ["devices/uart_tb.json"],
        "interface": "uart0"
      },
      "flash": {
        "@include@" : "devices/hyperflash.json",
        "cs": 1
      },
      "ram": {
        "@include@" : "devices/hyperram.json",
        "cs": 0
      },
      "jtag_proxy": {
        "@includes@": ["devices/jtag_proxy.json"]
      },
      "camera": {
        "@include@": "devices/camera.json",
        "interface": "cpi0",
        "ctrl_interface": "i2c1"
      }
    }
  },

  "chip": "wolfe",
  "chip_family": "wolfe",
    
  "padframe": {
    "content": "chips/wolfe/padframe.json"
  },

  "cluster": {
    "base": "0x10000000",
    "alias": "0x00000000",
    "size": "0x00400000",
    "core": "ri5ky_v2_fpu",
    "version": 5,
    "nb_cluster": 1,
    "nb_pe": 8,
    
    "pe": {
      "irq": [
        null       , null       , null         , null,
        null       , null       , null         , null,
        "dma_0"    , "dma_1"    , "timer_0"    , "timer_1",
        "acc_0"    , "acc_1"    , "acc_2"      , "acc_3",
        "eu_bar"   , "eu_mutex" , "eu_dispatch", null,
        null       , null       , "dma_ext"    , null,
        null       , null       , null         , null,
        "mpu_error", null       , "soc_event"  , "soc_fifo"
      ]
    },

    "l1": {
      "base": "0x10000000",
      "size": "0x00010000",
      "banking_factor": 2
    },

    "icache": {
      "config": {
        "nb_sets_bits": 6,
        "nb_ways_bits": 2,
        "line_size_bits": 4
      }
    },

    "peripherals": {

      "base": "0x10200000",
      "size": "0x00200000",

      "alias": {
        "base": "0x00200000",
        "size": "0x00200000"
      },

      "cluster_ctrl": {
        "base": "0x10200000",
        "size": "0x00000400"
      },

      "timer": {
        "base": "0x10200400",
        "size": "0x00000400"
      },

      "event_unit": {
        "base": "0x10200800",
        "size": "0x00000800"
      },

      "icache_ctrl": {
        "base": "0x10201400",
        "size": "0x00000400"
      },

      "dma": {
        "version": 6,
        "base": "0x10201800",
        "size": "0x00000400"
      },

      "dbg_unit": {
        "base": "0x10300000",
        "size"  : "0x00008000"
      },

      "cluster_ico": {}
    },

    "demux_peripherals":
    {
      "base": "0x10204000",

      "event_unit": {
        "base": "0x10204000",
        "size": "0x00000400"
      },
      "dma": {
        "base": "0x10204400",
        "size": "0x00000400"
      }
    }
  },

  "soc": {
    
    "base": "0x1A000000",
    "size": "0x06000000",

    "fc": {
      "core"        : "zeroriscy",
      "cluster_id"  : 31,
      "core_id"     : 0,
      "fetch_enable": true,
      "boot_addr"   : "0x1A000000"
    },

    "rom": {
      "version": 2,
      "base": "0x1A000000",
      "size": "0x00002000",
      "config": {
        "stim_file": "os.path.join(os.environ.get('INSTALL_DIR'), 'python', 'pulp', 'chips', 'wolfe', 'rom.bin')"
      }
    },

    "pulp_tap": {
      "config": {
        "@includes@": [ "ips/debug/pulp_tap.json" ]
      }
    },

    "taps": [ "pulp_tap" ],

    "rtc": {
      "version": 2
    },

    "l2": {
      "base": "0x1C000000",
      "size": "0x00080000",
      "is_partitioned": true,

      "priv0": {
        "base"         : "0x1C000000",
        "size"         : "0x00008000"
      },

      "priv0_alias": {
        "base"         : "0x00000000",
        "size"         : "0x00008000"
      },

      "priv1": {
        "base"         : "0x1C008000",
        "size"         : "0x00008000"
      },

      "shared": {
        "nb_banks"     : 4,
        "interleaving_bits" : 2,
        "base"         : "0x1C010000",
        "size"         : "0x00070000"
      }
    },

    "peripherals": {

      "base": "0x1A100000",
      "size": "0x00100000",

      "flls": {
        "fll": {
          "version"      : 1,
          "base"         : "0x1A100000",
          "size"         : "0x00000010",
          "targets"      : [ "soc", "periph" ]
        },

        "fll_cluster": {
          "version"      : 1,
          "base"         : "0x1A100010",
          "size"         : "0x00000010",
          "targets"      : [ "cluster" ]
        }
      },

      "fll_ctrl": {
        "version"      : 3
      },

      "gpio": {
        "version"      : 2,
        "base"         : "0x1A101000",
        "size"         : "0x00001000"
      },

      "udma": {
        "content"      : "chips/wolfe/udma.json",
        "base"         : "0x1A102000",
        "size"         : "0x00002000"
      },

      "apb_soc_ctrl": {
        "base"         : "0x1A104000",
        "size"         : "0x00001000",
        "has_jtag_reg" : true,
        "has_pmu_bypass" : true
      },

      "soc_eu": {
        "version"      : 1,
        "base"         : "0x1A106000",
        "size"         : "0x00001000"
      },

      "pmu": {
        "content"      : "chips/wolfe/pmu.json",
        "version"      : 2,
        "base"         : "0x1A107000",
        "size"         : "0x00000800"
      },

      "fc_itc": {
        "version"      : 1,
        "base"         : "0x1A109800",
        "size"         : "0x00000800",
        "irq": {
          "evt_sw_event0"       : 0,
          "evt_sw_event2"       : 2,
          "evt_sw_event1"       : 1,
          "evt_sw_event3"       : 3,
          "evt_sw_event4"       : 4,
          "evt_sw_event5"       : 5,
          "evt_sw_event6"       : 6,
          "evt_sw_event7"       : 7,

          "evt_timer0"          : 10,
          "evt_timer1"          : 11,

          "evt_clkref"          : 14,
          "evt_gpio"            : 15,
          "evt_rtc"             : 16,

          "evt_adv_timer0"      : 17,
          "evt_adv_timer1"      : 18,
          "evt_adv_timer2"      : 19,
          "evt_adv_timer3"      : 20,

          "evt_cluster_not_busy": 21,
          "evt_cluster_pok"     : 22,
          "evt_cluster_cg_ok"   : 23,
          "evt_picl_ok"         : 24,
          "evt_scu_ok"          : 25,

          "evt_soc_evt"         : 26,
          "evt_queue_error"     : 29
        }
      },

      "fc_timer": {
        "version"      : 2,
        "base"         : "0x1A10B000",
        "size"         : "0x00001000"
      },

      "stdout": {
        "version"      : 3,
        "base"         : "0x1A10F000",
        "size"         : "0x00001000"
      },

      "fc_dbg_unit": {
        "version"      : 1,
        "base"         : "0x1A110000",
        "size"         : "0x00008000"
      }

    }
  },

  "soc_events": {
    "soc_evt_spim0_rx"        : 0,
    "soc_evt_spim0_tx"        : 1,
    "soc_evt_spim1_rx"        : 2,
    "soc_evt_spim1_tx"        : 3,
    "soc_evt_hyper0_rx"       : 4,
    "soc_evt_hyper0_tx"       : 5,
    "soc_evt_uart0_rx"        : 6,
    "soc_evt_uart0_tx"        : 7,
    "soc_evt_i2c0_rx"         : 8,
    "soc_evt_i2c0_tx"         : 9,
    "soc_evt_i2c1_rx"         : 10,
    "soc_evt_i2c1_tx"         : 11,
    "soc_evt_i2s_ch0"         : 12,
    "soc_evt_i2s_ch1"         : 13,
    "soc_evt_cam0"            : 14,
    "soc_evt_spim0_eot"       : 22,
    "soc_evt_l2l2_eot"        : 23,
    "soc_evt_uart_eot"        : 25,
    "soc_evt_i2c0_extra"      : 26,
    "soc_evt_i2c1_extra"      : 27,
    "soc_evt_i2s_extra"       : 28,
    "soc_evt_cam0_eot"        : 29,
    "soc_evt_cluster_pok"     : 31,
    "soc_evt_cluster_not_busy": 34,
    "soc_evt_cluster_cg_ok"   : 35,
    "soc_evt_picl_ok"         : 36,
    "soc_evt_scu_ok"          : 37,
    "soc_evt_adv_timer0"      : 38,
    "soc_evt_adv_timer1"      : 39,
    "soc_evt_adv_timer2"      : 40,
    "soc_evt_adv_timer3"      : 41,
    "soc_evt_gpio"            : 42,
    "soc_evt_rtc_apb"         : 43,
    "soc_evt_rtc"             : 44,
    "soc_evt_ref_clock"       : 56,

    "soc_evt_sw_first"        : 48,
    "soc_evt_sw_nb"           : 8,

    "soc_evt_pmu0"            : 31
  }
}
