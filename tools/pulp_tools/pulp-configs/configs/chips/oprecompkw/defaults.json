{
  "runner": {
    "boot-mode": "loader"
  },
  
  "system_tree": {
    "board": {
      "chip": {
        "boot_from_rom": false,
        "soc": {
          "cluster": {
            "pe0": {
              "fetch_enable": false,
              "boot_addr": "0x1A000000"
            }
          }
        }
      }
    }
  },

  "loader": {
    "boot": {
      "mode": "jtag"
    },
    "set_pc_addr": "0x10200040",
    "set_pc_offset": "0",
    "start_addr": "0x10200008",
    "start_value": "0x1"
  }
}

