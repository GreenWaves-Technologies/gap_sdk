@INCLUDE = $(DOXTEMPLATE)/pulp-rt/pulp-rt.dxy

PROJECT_NAME           = AutoTiler

PROJECT_NUMBER         = "Version 1.0"

PROJECT_BRIEF          = "Auto Tiler Library"

# The OUTPUT_DIRECTORY tag is used to specify the (relative or absolute) path
# into which the generated documentation will be written. If a relative path is
# entered, it will be relative to the location where doxygen was started. If
# left blank the current directory will be used.

OUTPUT_DIRECTORY       = $(BUILD_DIR)/AUTOTILER

# The INPUT tag is used to specify the files and/or directories that contain
# documented source files. You may enter file names like myfile.cpp or
# directories like /usr/src/myproject. Separate the files or directories with
# spaces.
# Note: If this tag is empty the current directory is searched.

INPUT        = $(AUTOTILER_SRC)/GenTiling/AutoTilerLib.h $(AUTOTILER_SRC)/GenTiling/AutoTilerLibTypes.h
INPUT		 += $(AUTOTILER_GENERATOR_SRC)/HoG/HoGBasicKernels.h $(AUTOTILER_GENERATOR_SRC)/HoG/HoGGenerator.h $(AUTOTILER_EXAMPLES_SRC)/HoG/HoGModelExample.c
INPUT		 += $(AUTOTILER_EXAMPLES_SRC)/Fir/FirBasicKernels.h $(AUTOTILER_GENERATOR_SRC)/Fir/FirGenerator.h $(AUTOTILER_EXAMPLES_SRC)/Fir/FirModelExample.c
INPUT		 += $(AUTOTILER_GENERATOR_SRC)/CnnStdModel/CNN_BasicKernels.h $(AUTOTILER_GENERATOR_SRC)/CnnStdModel/CNN_HwCE.h $(AUTOTILER_GENERATOR_SRC)/CnnStdModel/CNN_Generator.h
INPUT        += $(AUTOTILER_EXAMPLES_SRC)/Cifar10/Cifar10Generator.c $(AUTOTILER_EXAMPLES_SRC)/Mnist/MnistGenerator.c
INPUT        += $(AT_BASE)/groups.h $(BUILD_DIR)/AUTOTILER/mainpage.md

@INCLUDE = $(COM_BASE)/common.dxy

HTML_EXTRA_FILES       += $(BUILD_DIR)/AUTOTILER/images.css
